{
  "name": "campzimmer-node",
  "version": "1.0.0",
  "description": "The Campzimmer Node library provides convenient access to the Campzimmer API from applications written in server-side JavaScript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup --config",
    "mocha": "nyc mocha",
    "test-debug": "mocha --inspect-brk",
    "test": "npm run mocha --debug",
    "lint-test": "npm run lint && npm run mocha",
    "lint": "eslint --ext .js, ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/campzimmer/campzimmer-node.git"
  },
  "keywords": [
    "campzimmer",
    "camping",
    "campground",
    "campsite",
    "api"
  ],
  "author": "Campzimmer, Inc. <support@campzimmer.com> (https://www.campzimmer.com)",
  "contributors": [
    "Kyle Kirst <kyle@campzimmer.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/campzimmer/campzimmer-node/issues"
  },
  "homepage": "https://github.com/campzimmer/campzimmer-node#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-prettier": "^3.1.0",
    "lodash.isplainobject": "^4.0.6",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rollup": "^1.15.5",
    "safe-buffer": "^5.1.2"
  }
}
